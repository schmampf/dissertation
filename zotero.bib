	langid = {english},
	file = {PDF:/Users/oliver/Documents/zotero/storage/2H24TRLE/Lorenz et al. - 2018 - Coulomb Blockade and Multiple Andreev Reflection in a Superconducting Single-Electron Transistor.pdf:application/pdf},

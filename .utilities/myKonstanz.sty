%%% Überschriften %%%
    % define new chapter
        \titleformat{\chapter}%
        {\selectfontsize{16pt}\sffamily\gdef\chapterlabel{}} 
        {\gdef\chapterlabel{}}
        {0pt}
        {
            \ifx\chapterlabel\empty\seeblauunderline{\textbf{#1}}\else\seeblauunderline{\textbf{\chapterlabel~{#1}}}\fi%
        } % before
        \titlespacing*{\chapter}{0pt}{28pt}{42pt}%

    % define new section
        \titleformat{\section}%
        {\selectfontsize{14pt}\sffamily\gdef\chapterlabel{}}%
        {\gdef\chapterlabel{\thesection}}
        {0pt}%
        {%
            \ifx\chapterlabel\empty\seeblauunderline{\textbf{#1}}\else\seeblauunderline{\textbf{\chapterlabel~#1}}\fi%
        }
        \titlespacing*{\section}{0pt}{25pt}{12.5pt}

    % define new subsection
        \titleformat{\subsection}
        {\selectfontsize{12pt}\sffamily\gdef\chapterlabel{}}
        {\gdef\chapterlabel{\thesubsection}}
        {0pt}
        {%
        \ifx\chapterlabel\empty\seeblauunderline{\textbf{#1}}\else\seeblauunderline{\textbf{\chapterlabel~#1}}\fi%
        }
        % \titlespacing*{\subsection}{0pt}{24pt}{12pt}
        \titlespacing*{\subsection}{0pt}{20pt}{10pt}

    % define new subsubsection
        \titleformat{\subsubsection}
        {\selectfontsize{9pt}\sffamily\gdef\chapterlabel{}}
        {}
        {0pt}
        {%
        \chapterlabel\empty\seeblauunderline{\textbf{#1}}%
        }
        \titlespacing*{\subsubsection}{0pt}{20pt}{10pt}



@article{lefeuvre_revealing_nodate,
	title = {Revealing topological helical edge states in the second order topological insulator Bi‚ÇÑBr‚ÇÑ},
	abstract = {One of the greatest recent achievements in condensed matter physics is the discovery of a new class of materials, topological insulators ({TIs}), whose bulk is insulating, while the edges conduct current in a quasi-ideal way. In particular, the 1D edges of 2D {TIs} realize the quantum spin Hall state, where current is carried without dissipation by two counter-propagating ballistic edge states, with spin orientation locked to the propagation direction (a helical edge state). This opens up a wide range of possibilities, from dissipation-free charge and spin transport at room temperature, to new avenues for quantum computing. The classification of {TIs} has recently been extended, with the discovery of second-order topological insulators ({SOTIs}), which are three-dimensional crystals whose bulk and surfaces are insulating, but which exhibit onedimensional conductive (topologically protected) helical 'hinge' states. Experimentally, only a few materials have been identified as {SOTIs}, such as bismuth and {WTe}2. However, most {SOTIs} also have conductive surfaces or bulk, so topological states coexist with non-topological ones. Recently, other materials have been proposed as belonging to this new family of topological insulators. Among them, Bi4Br4 is particularly interesting because its bulk and surfaces are truly insulating.},
	author = {Lefeuvre, Jules},
	langid = {english},
	file = {PDF:/Users/oliver/Documents/zotero/storage/M9BPJ3B2/Lefeuvre - Revealing topological helical edge states in the second order topological insulator Bi‚ÇÑBr‚ÇÑ.pdf:application/pdf},
}


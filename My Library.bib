@article{beenakker_random-matrix_1997,
	title = {Random-matrix theory of quantum transport},
	volume = {69},
	url = {https://link.aps.org/doi/10.1103/RevModPhys.69.731},
	doi = {10.1103/RevModPhys.69.731},
	abstract = {This is a review of the statistical properties of the scattering matrix of a mesoscopic system. Two geometries are contrasted: A quantum dot and a disordered wire. The quantum dot is a confined region with a chaotic classical dynamics, which is coupled to two electron reservoirs via point contacts. The disordered wire also connects two reservoirs, either directly or via a point contact or tunnel barrier. One of the two reservoirs may be in the superconducting state, in which case conduction involves Andreev reflection at the interface with the superconductor. In the case of the quantum dot, the distribution of the scattering matrix is given by either Dyson's circular ensemble for ballistic point contacts or the Poisson kernel for point contacts containing a tunnel barrier. In the case of the disordered wire, the distribution of the scattering matrix is obtained from the Dorokhov-Mello-Pereyra-Kumar equation, which is a one-dimensional scaling equation. The equivalence is discussed with the nonlinear ùúé model, which is a supersymmetric field theory of localization. The distribution of scattering matrices is applied to a variety of physical phenomena, including universal conductance fluctuations, weak localization, Coulomb blockade, sub-Poissonian shot noise, reflectionless tunneling into a superconductor, and giant conductance oscillations in a Josephson junction.},
	pages = {731--808},
	number = {3},
	journaltitle = {Reviews of Modern Physics},
	shortjournal = {Rev. Mod. Phys.},
	author = {Beenakker, C. W. J.},
	urldate = {2025-12-18},
	date = {1997-07-01},
	note = {Publisher: American Physical Society},
	file = {APS Snapshot:/Users/oliver/Documents/zotero/storage/F54AIPG4/RevModPhys.69.html:text/html;Full Text PDF:/Users/oliver/Documents/zotero/storage/ETDKU6FB/Beenakker - 1997 - Random-matrix theory of quantum transport.pdf:application/pdf},
}

